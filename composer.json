{
    "name": "webmaster/dompdf",
    "description": "Dompdf to greenter",
    "type": "library",
    "homepage": "https://github.com/ajahuanex/dompdf-greenter",
    "license": "MIT",
    "authors": [{
        "name": "Juan Carlos",
        "email": "ajahuana@hotmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "dompdf/dompdf": "^0.8.3",
        "greenter/core": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Webmaster\\": "src/"
        }
    },
    "config": {
        "vendor-dir": "vendor/greenter/"
    },

    "suggest": {
        "greenter/report": "Html Reporter"
    }
}
